{
  "name": "bracketEnum",
  "author": "aizigao",
  "version": "0.2.3",
  "keywords": ["enum"],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish --registry https://registry.npmjs.org/ --access public",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:w": "umi-test --watchAll",
    "test:coverage": "umi-test --coverage"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/aizigao/uni-select",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.10",
    "dumi": "^1.0.9",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.21",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "yorkie": "^2.0.0"
  }
}
